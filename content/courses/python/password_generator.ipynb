   "metadata": {},
   "outputs": [],
   "source": [
    "import random, string\n",
    "length = int(input('Password length: '))\n",
    "chars = string.ascii_letters + string.digits\n",
    "password = ''.join(random.choice(chars) for _ in range(length))\n",
    "print('Generated password:', password)\n"
   ]
  }
