   "source": [
    "# Quiz Program\n",
    "Answer questions and get your score."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "bdc5c32c",
   "metadata": {},
   "outputs": [],
   "source": [
    "questions = {'Capital of France?': 'Paris', '2 + 2?': '4'}\n",
    "score = 0\n",
    "for q, a in questions.items():\n",
    "    if input(q + ' ').strip().lower() == a.lower():\n",
    "        score += 1\n",
    "print('Score:', score, '/', len(questions))\n"
   ]
  }
